    "gitTag": "v1.0.9"
  },
  "scope": "organization",
  "components": {
    "services": [
      {
        "panelNames": [],
        "name": "app-service-none",
        "initParams": {},
        "runtime": {
          "podType": "regular-xs",
          "numReplicas": 0,
          "concurrency": 30
        },
        "maxAttempts": 13,
        "moduleName": "my-adapter",
        "modelOperation": "train",
        "operation": "none"
      },
      {
        "panelNames": [],
        "name": "app-service-install",
        "initParams": {
          "model_entity": "my-model-with-service"
        },
        "runtime": {
          "podType": "regular-xs",
          "numReplicas": 0,
          "concurrency": 20
        },
        "maxAttempts": 5,
        "moduleName": "my-adapter",
        "modelOperation": "deploy",
        "operation": "install"
      }
    ],
    "models": [
      {
        "name": "my-model-with-service",
        "moduleName": "my-adapter",
        "status": "trained",
        "operationType": "deploy"
      },
      {
        "name": "my-model-no-service",
        "moduleName": "my-adapter",
        "status": "trained",
        "operationType": "train"
      }
    ],
    "modules": [
      {
        "name": "my-adapter",
        "entryPoint": "main.py",
        "className": "ServiceRunner",
        "initInputs": [
          {
            "name": "model_entity",
            "type": "String"
          }
        ],
        "functions": [
          {
            "name": "evaluate_model",
            "input": [
              {
                "type": "Model",
                "name": "model"
              },
              {
                "type": "Dataset",
                "name": "dataset"
              },
              {
                "type": "Json",
                "name": "filters"
              }
            ],
            "output": [],
            "displayName": "Evaluate a Model",
            "displayIcon": ""
          },
          {
            "name": "predict_dataset",
            "input": [
              {
                "type": "Dataset",
                "name": "dataset"
              },
              {
                "type": "Json",
                "name": "filters"
              }
            ],
            "output": [],
            "displayName": "Predict Dataset with DQL",
            "displayIcon": ""
          },
          {
            "name": "predict_items",
            "input": [
              {
                "type": "Item[]",
                "name": "items"
              }
            ],
            "output": [
              {
                "type": "Item[]",
                "name": "items"
              },
              {
                "type": "Annotation[]",
                "name": "annotations"
              }
            ],
            "displayName": "Predict Items",
            "displayIcon": ""
          },
          {
            "name": "train_model",
            "input": [
              {
                "type": "Model",
                "name": "model"
              }
            ],
            "output": [
              {
                "type": "Model",
                "name": "model"
              }
            ],
            "displayName": "Train a Model",
            "displayIcon": ""
          }
        ]
      }
    ]
