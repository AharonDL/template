{
  "id": "2",
  "version": "1.0.9",
  "creator": "aharon@dataloop.ai",
  "name": "test-model-dpk",
  "displayName": "test-model-dpk",
  "description": "test-model-dpk",
  "icon": "icon-dl-sdk-documentation",
  "categories": [],
  "codebase": {
    "type": "git",
